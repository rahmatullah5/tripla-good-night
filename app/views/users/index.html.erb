<p style="color: green"><%= notice %></p>

<h1>Users</h1>

<div id="users">
  <table border="1">
  <th>ID</th>
  <th>Name</th>
  <th>Created At</th>
  <th>Updated At</th>
  <th>Actions</th>
  <% @users.each do |user| %>
    <tr id="<%= dom_id user %>">
      <%= render user %>
      <td><%= link_to "Show", user %></td>
    </tr>
  <% end %>
  </table>  
</div>
